{
  "name": "propagentic",
  "version": "27.0.0",
  "description": "Property management application with AI-powered maintenance request classification",
  "main": "index.js",
  "scripts": {
    "start": "react-app-rewired start",
    "build": "cross-env FAST_REFRESH=false DISABLE_ESLINT_PLUGIN=true GENERATE_SOURCEMAP=false NODE_OPTIONS=\"--max_old_space_size=4096 --openssl-legacy-provider\" react-app-rewired build --verbose",
    "vercel-build": "DISABLE_ESLINT_PLUGIN=true react-app-rewired build",
    "build:old": "cross-env NODE_OPTIONS=--openssl-legacy-provider react-app-rewired build",
    "build:fixed": "cross-env FAST_REFRESH=false DISABLE_ESLINT_PLUGIN=true GENERATE_SOURCEMAP=false NODE_OPTIONS=\"--max_old_space_size=4096 --openssl-legacy-provider\" react-app-rewired build",
    "build:clean": "cross-env NODE_OPTIONS=--openssl-legacy-provider TSC_COMPILE_ON_ERROR=true node build-fix.js",
    "build:debug": "cross-env NODE_OPTIONS=--openssl-legacy-provider react-app-rewired --inspect build",
    "build:safe": "cross-env DISABLE_ESLINT_PLUGIN=true CI=false GENERATE_SOURCEMAP=true NODE_OPTIONS=\"--max_old_space_size=4096 --openssl-legacy-provider\" react-scripts build --verbose",
    "build:openssl": "node scripts/build-openssl-fix.js",
    "test": "vitest",
    "test:unit": "vitest",
    "test:vitest": "vitest run",
    "test:vitest:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "test:integration": "vitest run src/test/integration/",
    "test:unit-only": "vitest run --exclude src/test/integration/",
    "test:with-emulators": "firebase emulators:exec --project=demo \"npm run test:coverage\"",
    "test:analytics": "node tests/run-tests.js all",
    "test:analytics:unit": "node tests/run-tests.js unit",
    "test:analytics:integration": "node tests/run-tests.js integration",
    "test:analytics:component": "node tests/run-tests.js component",
    "test:analytics:services": "node tests/run-tests.js services",
    "test:analytics:quick": "node tests/run-tests.js quick",
    "test:analytics:coverage": "npx vitest run --coverage tests/analytics/",
    "test:analytics:watch": "npx vitest tests/analytics/ --watch",
    "test:analytics:ui": "npx vitest --ui tests/analytics/",
    "test:analytics:perf": "node tests/run-tests.js all --reporter=verbose --performance",
    "test:invite:create": "node scripts/test-invite-code-creation.js",
    "test:invite:redeem": "node scripts/test-invite-code-redemption.js",
    "test:invite": "node scripts/test-invite-code-creation.js && node scripts/test-invite-code-redemption.js",
    "eject": "react-scripts eject",
    "typecheck": "npx tsc --noEmit",
    "lint": "eslint src --ext .js,.jsx,.ts,.tsx",
    "start:safe": "cross-env DISABLE_ESLINT_PLUGIN=true CHOKIDAR_USEPOLLING=true react-app-rewired start",
    "start:fix": "cross-env FAST_REFRESH=false DISABLE_ESLINT_PLUGIN=true CHOKIDAR_USEPOLLING=true react-app-rewired start",
    "start:legacy": "cross-env NODE_OPTIONS=--openssl-legacy-provider DISABLE_FORK_TS_CHECKER=true CHOKIDAR_USEPOLLING=true react-scripts start",
    "clear-cache": "rm -rf node_modules/.cache",
    "clean-start": "npm run clear-cache && npm run start:safe",
    "deploy": "npm run build && firebase deploy --only hosting",
    "deploy:clean": "npm run build:clean && firebase deploy --only hosting",
    "deploy:firebase": "npm run build && firebase deploy --only hosting",
    "deploy:helper": "node deploy-helper.js",
    "deploy:force": "node deploy-helper.js --force",
    "deploy:rules": "firebase deploy --only firestore:rules",
    "integration-test": "node integration-test.js",
    "test:integration": "node integration-test.js",
    "performance-test": "node -e \"require('./src/utils/performanceTests').runTestsInBrowser()\"",
    "check-browser-compatibility": "node -e \"require('./browserCompatibilityTest')\"",
    "prepare-deploy": "npm run typecheck && npm run lint && npm run test && npm run integration-test && npm run build",
    "port-check": "node port-manager.js",
    "start:port": "node port-manager.js && npm run start",
    "start:alt": "cross-env PORT=3001 DISABLE_ESLINT_PLUGIN=true DISABLE_FORK_TS_CHECKER=true react-app-rewired start",
    "setup:playwright": "node setup-playwright.js",
    "rebuild": "rm -rf node_modules package-lock.json && npm install --legacy-peer-deps && npm run build:safe",
    "clean": "rimraf node_modules && rimraf package-lock.json",
    "reinstall": "npm run clean && npm cache clean --force && npm install",
    "fix-router": "npm uninstall react-router-dom react-router && npm install react-router-dom@6.3.0 --legacy-peer-deps",
    "fix-framer": "npm uninstall framer-motion && npm install framer-motion@latest --legacy-peer-deps",
    "fix-react": "npm uninstall react react-dom && npm install react@latest react-dom@latest --legacy-peer-deps",
    "fix-types": "npm uninstall @types/react @types/react-dom && npm install @types/react@latest @types/react-dom@latest --legacy-peer-deps",
    "fix-all-deps": "npm run fix-router && npm run fix-framer && npm run fix-react && npm run fix-types",
    "migrate-framer-motion": "node scripts/migrate-framer-motion.js",
    "clean:force": "sudo rm -rf node_modules package-lock.json",
    "fix:deps": "npm run clean:force && node scripts/fix-dependencies.js"
  },
